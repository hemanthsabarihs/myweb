pipeline {
    agent any
    environment {
        PATH = "/opt/maven3/bin:$PATH"
    }
    stages {
        stage('GIT SCM CHECKOUT') {
            steps {
               git 'https://github.com/hemanthsabarihs/myweb.git'
            }
        }
        stage('MAVEN BUILD PACKAGE') {
            steps {
               sh "mvn clean package"
            }
        }        
    }
}
